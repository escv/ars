<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.text.*?>

<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" id="ACTAnchorPane" fx:controller="com.prodyna.pac.ars.frontend.fxml.AircraftTypeController" >
    <Label styleClass="title" text="Aircraft Type Management"/>
     
    <TableView fx:id="entryTable">
        <columns>
              <TableColumn text="Name" prefWidth="100">
                  <cellValueFactory>
                      <PropertyValueFactory property="name" />
                  </cellValueFactory>
              </TableColumn>
          </columns>
    </TableView>
    <Button onAction="#showCreateForm" text="New Aircraft Type"/>
</VBox>
